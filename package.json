  "version": "0.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update": "ng update @angular/cli @angular/core @angular/material @angular-eslint/schematics",
    "version": "node scripts/version.js && git add ./src/environments/version.ts",
